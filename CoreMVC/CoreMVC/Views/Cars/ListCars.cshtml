    <h1>All cars:</h1>
    <h3>@Model.currCategory</h3>
    @{
        foreach (var car in Model.AllCars)
        {
            <div>
                <h2>
                    Model: @car.Name
                </h2>
                <p>
                    Sort description: @car.ShortDesc
                    Description: @car.LongDesc
                    Price: @car.Price.ToString()
                </p>

            </div>
        }
    }

